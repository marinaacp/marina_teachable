<div class="container mt-4">
  <h1 class="mb-4">Enrollments</h1>

  <div class="container-fluid">
    <table class="table js-dataTable-full">
      <thead>
        <tr>
          <th>Name</th>
          <th>EMAIL</th>
        </tr>
      </thead>
      <tbody>
        <% @enrollments_data.each do |e| %>
          <tr>
            <td><%= e[:name] %></td>
            <td><%= e[:email]  %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
